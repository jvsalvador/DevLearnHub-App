<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".module.Java_Module5_6">

    <TextView
        android:id="@+id/textView247"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Java Stack Class"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView256"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView247"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="The Java collections framework has a class named Stack that provides the functionality of the stack data structure."
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView258"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView256"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="The Stack class extends the Vector class."
        android:textSize="12sp" />

    <ImageView
        android:id="@+id/imageView36"
        android:layout_width="300dp"
        android:layout_height="200dp"
        android:layout_below="@+id/textView258"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        app:srcCompat="@drawable/java_6" />

    <TextView
        android:id="@+id/textView259"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView36"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Stack Implementation"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView260"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView259"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="In stack, elements are stored and accessed in Last In First Out manner. That is, elements are added to the top of the stack and removed from the top of the stack."
        android:textSize="12sp" />

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="300dp"
        android:layout_height="200dp"
        android:layout_below="@+id/textView260"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        app:srcCompat="@drawable/java_7" />

    <TextView
        android:id="@+id/btn_tap_to_next34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp"
        android:fontFamily="@font/poppins_regular"
        android:text="All Done!"
        android:textSize="16sp" />

</RelativeLayout>