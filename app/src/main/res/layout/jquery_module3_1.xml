<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".module.Jquery_Module3_1">

    <TextView
        android:id="@+id/textView385"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/poppins_bold"
        android:text="R Strings"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView386"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView385"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="A string is a sequence of characters. For example, &quot;Programming&quot; is a string that includes characters: P, r, o, g, r, a, m, m, i, n, g.\n\nIn R, we represent strings using quotation marks (double quotes, &quot; &quot; or single quotes, ' ').\n\nFor example,"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView387"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView386"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:background="@color/black"
        android:text="\n    # string value using single quotes\n    'Hello'\n\n    # string value using double quotes\n    &quot;Hello&quot;\n"
        android:textColor="@color/white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView388"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView387"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="Here, both 'Hello' and &quot;Hello&quot; are strings. We can use either single quotes or double quotes to represent strings in R.\n\nHowever, we cannot mismatch them. For example, start the string with a single quote and end it with a double quote, 'Hello&quot; and vice versa, &quot;Hello'."
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView389"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView388"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Example: Strings in R"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView390"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView389"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:background="@color/black"
        android:text="\n    message1 &lt;- 'Hola Amigos'\n    print(message1)\n\n    message2 &lt;- &quot;Welcome to Programiz&quot;\n    print(message2)\n"
        android:textColor="@color/white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView761"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView390"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Output  "
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView762"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView761"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:background="@color/black"
        android:text="\n    [1] &quot;Hola Amigos&quot;\n    [1] &quot;Welcome to Programiz&quot;\n"
        android:textColor="@color/white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/jquery_tap_to_next26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Tap to continue"
        android:textSize="16sp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="10dp"
        app:srcCompat="@drawable/baseline_arrow_back_ios_24" />
</RelativeLayout>