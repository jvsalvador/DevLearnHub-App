<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".module.Java_Module6_3">

    <TextView
        android:id="@+id/textView274"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Java LinkedList"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView275"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView274"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="The LinkedList class of the Java collections framework provides the functionality of the linked list data structure (doubly linkedlist)."
        android:textSize="12sp" />

    <ImageView
        android:id="@+id/imageView40"
        android:layout_width="500dp"
        android:layout_height="200dp"
        android:layout_below="@+id/textView275"
        android:layout_marginTop="10dp"
        app:srcCompat="@drawable/java_9" />

    <TextView
        android:id="@+id/textView276"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView40"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/poppins_bold"
        android:text="Creating a Java LinkedList"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView277"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView276"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="Here is how we can create linked lists in Java:"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView278"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView277"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:background="@color/black"
        android:text="\n    LinkedList<Type> linkedList = new LinkedList<>();\n"
        android:textColor="@color/white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView279"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView278"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:text="Here, Type indicates the type of a linked list. For example,"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView280"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView279"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:background="@color/black"
        android:text="\n    // create Integer type linked list\n    LinkedList<Integer> linkedList = new LinkedList<>();\n\n    // create String type linked list\n    LinkedList<String> linkedList = new LinkedList<>();\n"
        android:textColor="@color/white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/btn_tap_to_next37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Tap to continue"
        android:textSize="16sp" />
</RelativeLayout>